#include <stdio.h>
#include <string.h>
#include <math.h>

int main() {
    char bin[50];
    scanf("%s", bin);

    int dec = 0, len = strlen(bin);
    for (int i = 0; i < len; i++) {
        if (bin[i] == '1') {
            dec += pow(2, len - i - 1);
        }
    }

    printf("Decimal: %d\n", dec);

    return 0;
}

/*
Input:
1010

Output:
Decimal: 10
*/
